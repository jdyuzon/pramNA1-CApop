				</stdev>
			</logNormalDistributionModel>
		</distribution>
		<rateCategories>
			<parameter id="sv.branchRates.categories"/>
		</rateCategories>
	</discretizedBranchRates>
	<rateStatistic id="sv.meanRate" name="sv.meanRate" mode="mean" internal="true" external="true">
